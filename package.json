    "autoprefixer": "^10.4.7",
    "child_process": "^1.0.2",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.2.0",
    "postcss": "^8.4.14",
