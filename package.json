    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-eslint-parser": "^0.16.4",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
